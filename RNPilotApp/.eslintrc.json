{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/forbid-prop-types": [
      0
    ],
    "react/require-default-props": [
      0
    ],
    "global-require": [
      0
    ],
    "react/prop-types": 0,
    "react/sort-comp": 0,
    "radix": 0,
    "no-plusplus": 0,
    "no-cond-assign": 0,
    "no-param-reassign": 0,
    "no-alert": 0
  }
}